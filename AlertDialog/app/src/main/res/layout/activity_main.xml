<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <Button
        android:onClick="RateMeDialog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Rate Me Dialog " />

    <Button
        android:onClick="NotDismissAlertDialog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Not Dismiss AlertDialog" />

    <Button
        android:onClick="OneButtonAlertDialog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="One Button AlertDialog" />

    <Button
        android:onClick="PassingEventAlertDialog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Passing Event AlertDialog" />

</LinearLayout>