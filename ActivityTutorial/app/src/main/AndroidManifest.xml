<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.issam.drmas.activitytutorial">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PassVariablesActivity" />
        <activity
            android:name=".Main2Activity"
            android:parentActivityName=".PassVariablesActivity" />
        <activity android:name=".StartActivityForResultActivity" />
        <activity android:name=".ReservestartActivityForResultActivity"></activity>
    </application>

</manifest>